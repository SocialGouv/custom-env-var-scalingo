#!/bin/bash

set -e
set -o pipefail

test ! -d ".profile.d" && mkdir -p .profile.d || true

cat > ".profile.d/zidane.sh" <<SH
echo "PO PO POOOOOO"
echo $CUSTOM_YO
echo "HOPE"
export CUSTOM_YO="YOOOOOOOO"
export YO="PLOOOOOOO"
echo $YO
echo "HIP"
SH

cat .profile.d/zidane.sh

source ".profile.d/zidane.sh"
